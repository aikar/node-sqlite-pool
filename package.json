{
  "private": true,
  "name": "sqlite-pool",
  "version": "0.1.0",
  "description": "Pooled SQLite client for Node.js with migrations and transactions, based on node-sqlite",
  "repository": "rneilson/node-sqlite-pool",
  "author": "Raymond Neilson <rneilson@interloper.ca> (https://www.interloper.ca)",
  "contributors": [
    "Kriasoft <hello@kriasoft.com> (http://www.kriasoft.com)",
    "Konstantin Tarkus <hello@tarkus.me> (https://twitter.com/koistya)"
  ],
  "keywords": [
    "sqlite",
    "db",
    "database",
    "sql",
    "migrate",
    "migration",
    "migrations",
    "migrator",
    "seed",
    "seeds",
    "seeder",
    "node",
    "node5",
    "babel",
    "es6",
    "async",
    "promise",
    "promises",
    "api",
    "pool",
    "transactions",
    "transaction"
  ],
  "license": "MIT",
  "main": "main.js",
  "jsnext:main": "main.mjs",
  "types": "main.d.ts",
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "rules": {
      "no-restricted-syntax": "off",
      "prefer-rest-params": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "dependencies": {
    "generic-pool": "^3.1.6",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "node tools/build && mocha",
    "build": "node tools/build"
  }
}
